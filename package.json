{
  "name": "mflisikowski-portfolio-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ssl": "npx local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:migrate:reset": "npx prisma migrate reset",
    "prisma:migrate:dev": "npx prisma migrate dev --name $1",
    "prisma:generate": "npx prisma generate",
    "prisma:backup": "node prisma-backup.mjs",
    "prisma:seed": "node prisma-seeds.mjs",
    "prisma:seed:employee": "node prisma/seed/employee.mjs",
    "prisma:seed:availability": "node prisma/seed/availability.mjs",
    "prisma:seed:relocation": "node prisma/seed/relocation.mjs",
    "prisma:seed:status": "node prisma/seed/status.mjs",
    "prisma:seed:company": "node prisma/seed/company.mjs",
    "prisma:seed:workplace": "node prisma/seed/workplace.mjs",
    "prisma:seed:route": "node prisma/seed/route.mjs",
    "prisma:seed:page": "node prisma/seed/page.mjs",
    "prisma:seed:meta": "node prisma/seed/meta.mjs",
    "prisma:seed:category": "node prisma/seed/category.mjs",
    "prisma:seed:use": "node prisma/seed/use.mjs",
    "prisma:seed:setting": "node prisma/seed/setting.mjs",
    "prisma:seed:social": "node prisma/seed/social.mjs",
    "prisma:seed:skill": "node prisma/seed/skill.mjs",
    "prisma:studio": "npx prisma studio",
    "purge:modules": "rm -rf node_modules",
    "purge:next": "rm -rf .next"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@prisma/client": "^4.8.1",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.12",
    "cal-sans": "^1.0.1",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^1.2.1",
    "focus-visible": "^5.2.0",
    "moment": "^2.29.4",
    "next": "13.0.2",
    "octokit": "^2.0.10",
    "postcss-focus-visible": "^6.0.4",
    "prisma": "^4.8.1",
    "prop-types": "^15.8.1",
    "puppeteer": "^19.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-downloader": "^1.2.1",
    "tailwindcss": "^3.2.1"
  },
  "devDependencies": {
    "@vorlefan/prisma-backup": "^0.3.5",
    "await-spawn": "^4.0.2",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "node-color-log": "^10.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "^4.4.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "uuid4": "^2.0.3"
  }
