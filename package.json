{
  "name": "mflisikowski-portfolio-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npx prisma migrate deploy",
    "prisma:seed:workplace": "node prisma/seeds/workplace.mjs",
    "prisma:seed:category": "node prisma/seeds/category.mjs",
    "prisma:seed:company": "node prisma/seeds/company.mjs",
    "prisma:seed:setting": "node prisma/seeds/setting.mjs",
    "prisma:seed:social": "node prisma/seeds/social.mjs",
    "prisma:seed:route": "node prisma/seeds/route.mjs",
    "prisma:seed:page": "node prisma/seeds/page.mjs",
    "prisma:seed:meta": "node prisma/seeds/meta.mjs",
    "prisma:seed:use": "node prisma/seeds/use.mjs",
    "prisma:migrate": "npx prisma migrate dev --name $1",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "local-ssl": "npx local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "postinstall": "node install.mjs"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@prisma/client": "4.8.0",
    "@supabase/supabase-js": "^2.2.1",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.12",
    "cal-sans": "^1.0.1",
    "clsx": "^1.2.1",
    "focus-visible": "^5.2.0",
    "next": "13.0.2",
    "octokit": "^2.0.10",
    "postcss-focus-visible": "^6.0.4",
    "prisma": "4.8.0",
    "prop-types": "^15.8.1",
    "puppeteer": "^19.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-downloader": "^1.2.1",
    "tailwindcss": "^3.2.1"
  },
  "devDependencies": {
    "await-spawn": "^4.0.2",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "node-color-log": "^10.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "^4.4.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "uuid4": "^2.0.3"
  }
